{
  "name": "@kiiako/blocks",
  "version": "1.1.0",
  "description": "Pre-built blocks for Kiiako page builder",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./auto-register": {
      "types": "./dist/auto-register.d.ts",
      "import": "./dist/auto-register.mjs",
      "require": "./dist/auto-register.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kiiako",
    "blocks",
    "react",
    "components",
    "page-builder"
  ],
  "author": "Kiiako Team",
  "license": "MIT",
  "peerDependencies": {
    "@kiiako/render": "^1.0.0",
   "react": "^18.0.0 || ^19.0.0",
   "react-dom": "^18.0.0 || ^19.0.0"

  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@kiiako/render": "file:../render",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiiako/kiiako-blocks"
  }
}
