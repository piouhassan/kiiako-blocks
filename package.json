{
  "name": "@kiiako/blocks",
  "version": "1.0.0",
  "description": "Pre-built blocks for Kiiako page builder",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./auto-register": {
      "types": "./src/auto-register.d.ts",
      "import": "./src/auto-register.ts",
      "require": "./src/auto-register.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kiiako",
    "blocks",
    "react",
    "components",
    "page-builder"
  ],
  "author": "Kiiako Team",
  "license": "MIT",
  "peerDependencies": {
    "@kiiako/render": ">=1.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lucide-react": "^0.263.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kiiako/render": "file:../render",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiiako/kiiako-blocks"
  }
}
